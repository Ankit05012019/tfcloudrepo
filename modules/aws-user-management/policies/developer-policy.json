{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "FullStackS3Access",
      "Action": [
        "s3:Get*",
        "s3:List*"
      ],
      "Effect": "Allow",
      "Resource": "*"
    },
    {
      "Sid": "FullStackRDSAccess",
      "Action": "rds:Describe*",
      "Effect": "Allow",
      "Resource": "*"
    },
    {
      "Sid": "FullStackEC2Access",
      "Effect": "Allow",
      "Action": "ec2:Describe*",
      "Resource": "*"
    },
    {
      "Sid": "FullStackACMAccess",
      "Effect": "Allow",
      "Action": "acm:List*",
      "Resource": "*"
    },
    {
      "Sid": "FullStackECSAccess",
      "Effect": "Allow",
      "Action": [
        "ecs:List*",
        "ecs:Describe*"
      ],
      "Resource": "*"
    },
    {
      "Sid": "FullStackCloudFormationAccess",
      "Effect": "Allow",
      "Action": "cloudformation:Describe*",
      "Resource": "*"
    },
    {
      "Sid": "FullStackAutoScalingAccess",
      "Effect": "Allow",
      "Action": "autoscaling:Describe*",
      "Resource": "*"
    },
    {
      "Sid": "FullStackKMSAccess",
      "Effect": "Allow",
      "Action": [
        "kms:List*",
        "kms:Describe*"
      ],
      "Resource": "*"
    },
    {
      "Sid": "FullStackOpsWorksAccess",
      "Effect": "Allow",
      "Action": "opsworks:Describe*",
      "Resource": "*"
    },
    {
      "Sid": "FullStackELBAccess",
      "Effect": "Allow",
      "Action": [
        "elasticloadbalancing:List*",
        "elasticloadbalancing:Describe*"
      ],
      "Resource": "*"
    },
    {
      "Sid": "FullStackCloudFrontAccess",
      "Effect": "Allow",
      "Action": "cloudfront:List*",
      "Resource": "*"
    },
    {
      "Sid": "FullStackElasticCacheAccess",
      "Effect": "Allow",
      "Action": "elasticcache:Describe*",
      "Resource": "*"
    },
    {
      "Sid": "FullStackCloudWatchAccess",
      "Effect": "Allow",
      "Action": [
        "cloudwatch:Describe*",
        "cloudwatch:List*",
        "cloudwatch:Get*",
        "events:Describe*",
        "events:List*",
        "logs:Describe*",
        "logs:List*",
        "health:Describe*"
      ],
      "Resource": "*"
    }
  ]
}